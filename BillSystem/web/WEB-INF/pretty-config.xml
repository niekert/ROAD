<pretty-config xmlns="http://ocpsoft.org/schema/rewrite-config-prettyfaces"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://ocpsoft.org/schema/rewrite-config-prettyfaces
                      http://ocpsoft.org/xml/ns/prettyfaces/rewrite-config-prettyfaces.xsd">


    <url-mapping>
        <pattern value="/login/"></pattern>
        <view-id value="../login.xhtml"/>
    </url-mapping>
    <url-mapping>
        <pattern value="/invoicegeneration/"></pattern>
        <view-id value="../WEB-INF/pages/invoicegeneration.xhtml"/>
    </url-mapping>
    <url-mapping>
        <pattern value="/invoicesearch/"></pattern>
        <view-id value="../WEB-INF/pages/invoiceSearcher.xhtml"/>
    </url-mapping>
    <url-mapping>
        <pattern value="/invoicedetail/#{invoiceID}"></pattern>
        <view-id value="../WEB-INF/pages/invoiceDetailsPage.xhtml"/>
    </url-mapping>
    <url-mapping>
        <pattern value="/manageCars/"></pattern>
        <view-id value="../WEB-INF/pages/cartrackerManager.xhtml"/>
    </url-mapping>
    <url-mapping>
        <pattern value="/adjustRate/"></pattern>
        <view-id value="../WEB-INF/pages/adjust_km_rate.xhtml"/>
    </url-mapping>

</pretty-config>